{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb",
		"prettier"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"parser": "babel-eslint",
	"plugins": [
		"import",
		"react",
		"jsx-a11y",
		"babel"
	],
	"rules": {
		"linebreak-style": [
			"error",
			"unix"
		],
		"semi": [
			"error",
			"never"
		],
		"no-console": "off",
		"react/jsx-filename-extension": [1, {
			"extensions": [".js", ".jsx"]
		}],
		"import/no-unresolved": 0
	}
}
